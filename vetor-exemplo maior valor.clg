algoritmo semNome;
// Síntese
//  Objetivo:  
//  Entrada :
//  Saída   :


principal
	// Declarações
inteiro vetor [10];
inteiro i;
inteiro posicao;

	// Instruções

para (i de 0 ate 9 passo 1) faca
escreval("entre com o elemento ", i," do vetor.  " );
leia (vetor[i]);
fimPara

posicao = 0;

para (i de 1 ate 9 passo 1) faca
	se (vetor[i] > vetor [posicao]) entao
	posicao = i;
	fimSe
	fimPara
	escreval ("O maior elemento do vetor = ", posicao);
	

fimPrincipal
