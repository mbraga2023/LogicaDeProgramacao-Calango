algoritmo vetor;
// Síntese
//  Objetivo:  
//  Entrada :
//  Saída   :


principal
	// Declarações
	real notas [5], soma;
	inteiro i;
	// Instruções
	soma = 0;
	para (i de 0 ate 4 passo 1) faca
	
		escreva ("Informe a ", i+1, " nota: ");
		leia (notas[i]);
		soma = soma + notas[i]; 
	
	fimPara

	escreval ("A soma das notas é ", soma);
	escreval ("A média da turma é : ", (soma/i):3:1);
	
fimPrincipal
