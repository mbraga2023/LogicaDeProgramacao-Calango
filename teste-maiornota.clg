algoritmo maior;
// Síntese
//  Objetivo:  Encontrar a maior nota
//  Entrada : 5 notas
//  Saída   : maior nota


principal
	// Declarações

	real nota, maior;
	inteiro i;

	maior = -1; // inicialização com valor absurdo
	
	// Instruções

	para (i de 1 ate 5 passo 1) faca
		escreva ("Informe a ", i, " nota: ");
		leia (nota);
		se (nota > maior) entao
			maior = nota;
		fimSe
	fimPara
	escreva ("A maior nota é ", maior);
	
	
fimPrincipal
